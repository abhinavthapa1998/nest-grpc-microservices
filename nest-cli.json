{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-grpc-microservices/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-grpc-microservices/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-grpc-microservices",
  "projects": {
    "nest-grpc-microservices": {
      "type": "application",
      "root": "apps/nest-grpc-microservices",
      "entryFile": "main",
      "sourceRoot": "apps/nest-grpc-microservices/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-grpc-microservices/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    }
  }
}